<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/fundoNeutro">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:paddingBottom="@dimen/paddingContainerLicao"
        android:paddingEnd="@dimen/paddingContainerLicao"
        android:paddingLeft="@dimen/paddingContainerLicao"
        android:paddingRight="@dimen/paddingContainerLicao"
        android:paddingStart="@dimen/paddingContainerLicao">

        <TextView
            android:id="@+id/pergunta"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="15dp"
            android:fontFamily="@string/fonteQuestao"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/ProvaModulo1Pergunta2"
            android:textColor="@color/corTituloLicao"
            android:textSize="15sp"
            android:textStyle="normal|bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/ProvaModulo1Pergunta2Linha1"
            android:text="@string/ProvaModulo1Pergunta2Linha1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/corTituloLicao"
            android:textSize="12.5sp"
            android:typeface="normal"
            android:fontFamily="@string/fonteQuestao"
            android:layout_marginBottom="5dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal">

            <TextView
                android:text="@string/ProvaModulo1Pergunta2Linha2"
                android:id="@+id/ProvaModulo1Pergunta2Linha2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>

            <EditText
                android:id="@+id/palavra1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:textSize="13sp"
                android:layout_weight="15"
                android:maxLength="7"
                android:maxLines="1"
                android:width="75dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal">

            <TextView
