<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.tcc.dagon.opus.ContainerLicoes.Modulos.Provas.ContainerProva3"
    >

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbarModulo3Prova"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

        <android.support.design.widget.TabLayout
            android:id="@+id/tab_layout_modulo3_prova"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/corTabProva" />
    </android.support.design.widget.AppBarLayout>



    <include layout="@layout/vidas_provas"/>
    <include layout="@layout/content_modulo3_prova"/>

</android.support.design.widget.CoordinatorLayout>
