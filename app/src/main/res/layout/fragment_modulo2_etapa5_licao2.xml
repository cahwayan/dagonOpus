<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#e7e7e7">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/paddingContainerLicao"
        android:layout_margin="5dp"
        >
        <TextView
            android:id="@+id/Modulo2Etapa5Pergunta1"
            android:text="@string/Modulo2Etapa5Pergunta1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:textColor="@color/corTituloLicao"
            android:textSize="15sp"
            android:textStyle="normal|bold"
            android:typeface="normal"
            android:fontFamily="@string/fonteQuestao"
            android:layout_marginBottom="15dp" />

        <TextView
            android:id="@+id/Modulo2Etapa5Pergunta1Linha0"
            android:text="@string/Modulo2Etapa5Pergunta1Linha0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/corTituloLicao"
            android:textSize="12.5sp"
            android:typeface="normal"
            android:fontFamily="@string/fonteQuestao"
            android:layout_marginBottom="5dp"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal|fill_horizontal"
        android:layout_marginTop="15dp">

        <EditText
            android:id="@+id/Modulo2Etapa5Pergunta1Linha1Palavra1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:textSize="13sp"
            android:layout_weight="15"
            android:maxLength="6"
            android:maxLines="1"
            android:width="45dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal"
            android:layout_marginTop="15dp">

            <TextView
                android:text="@string/Modulo2Etapa5Pergunta1Linha1"
                android:id="@+id/Modulo2Etapa5Pergunta1Linha1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal"
            android:layout_marginTop="15dp">

            <TextView
                android:text="@string/Modulo2Etapa5Pergunta1Linha2"
                android:id="@+id/Modulo2Etapa5Pergunta1Linha2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>

            <EditText
                android:id="@+id/Modulo2Etapa5Pergunta1Linha2Palavra1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:textSize="13sp"
                android:layout_weight="15"
                android:maxLength="3"
                android:maxLines="1"
                android:width="35dp" />

            <TextView
                android:text=")"
                android:id="@+id/hardcode6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal"
            android:layout_marginTop="5dp">

            <EditText
                android:id="@+id/Modulo2Etapa5Pergunta1Linha3Palavra1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:textSize="13sp"
                android:layout_weight="15"
                android:maxLength="4"
                android:maxLines="1"
                android:width="45dp"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="15dp" />

            <TextView
                android:text="@string/Modulo2Etapa5Pergunta1Linha3"
                android:id="@+id/Modulo2Etapa5Pergunta1Linha3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal"
            android:layout_marginTop="5dp">

            <TextView
                android:text="@string/Modulo2Etapa5Pergunta1Linha4"
                android:id="@+id/Modulo2Etapa5Pergunta1Linha4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal"
            android:layout_marginTop="5dp">

            <TextView
                android:text="@string/Modulo2Etapa5Pergunta1Linha5"
                android:id="@+id/Modulo2Etapa5Pergunta1Linha5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>

            <EditText
                android:id="@+id/Modulo2Etapa5Pergunta1Linha5Palavra1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:textSize="13sp"
                android:layout_weight="15"
                android:maxLength="9"
                android:maxLines="1"
                android:width="95dp" />

            <TextView
                android:text=")"
                android:id="@+id/hardcode7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal"
            android:layout_marginTop="5dp">

            <TextView
                android:text="@string/Modulo2Etapa5Pergunta1Linha6"
                android:id="@+id/Modulo2Etapa5Pergunta1Linha6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal"
            android:layout_marginTop="5dp">

            <TextView
                android:text="@string/Modulo2Etapa5Pergunta1Linha7"
                android:id="@+id/Modulo2Etapa5Pergunta1Linha7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>

            <EditText
                android:id="@+id/Modulo2Etapa5Pergunta1Linha7Palavra1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:textSize="13sp"
                android:layout_weight="15"
                android:maxLength="4"
                android:maxLines="1"
                android:width="45dp" />

            <TextView
                android:text=")"
                android:id="@+id/hardcode8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12.5sp"
                android:gravity="start|end"
                android:layout_gravity="end|center_vertical"
                android:paddingTop="@dimen/paddingTopoTextoLicao"
                android:paddingBottom="@dimen/paddingBaixoTextoLicao"
                android:lineSpacingExtra="2sp"
                android:typeface="normal"/>
        </LinearLayout>

        <include layout="@layout/botoes_responder_questao"/>
    </LinearLayout>

</ScrollView>