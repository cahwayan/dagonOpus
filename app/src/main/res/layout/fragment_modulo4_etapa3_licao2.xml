            <TextView
                android:id="@+id/Modulo4Etapa3Pergunta1Linha12"
                android:text="@string/Modulo4Etapa3Pergunta1Linha6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/corTituloLicao"
                android:textSize="12.5sp"
                android:typeface="normal"
                android:fontFamily="@string/fonteQuestao"
                android:layout_marginBottom="5dp"/>

        </LinearLayout>

        <TextView
            android:id="@+id/Modulo4Etapa3Pergunta1Linha13"
            android:text="@string/Modulo4Etapa3Pergunta1Linha13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/corTituloLicao"
            android:textSize="12.5sp"
            android:typeface="normal"
            android:fontFamily="@string/fonteQuestao"
            android:layout_marginBottom="5dp"/>



        <include layout="@layout/botoes_responder_questao"/>
    </LinearLayout>

</ScrollView>