            android:layout_height="wrap_content"
            android:textColor="@color/corTituloLicao"
            android:textSize="12.5sp"
            android:typeface="normal"
            android:fontFamily="@string/fonteQuestao"
            android:layout_marginBottom="5dp"/>

        <TextView
            android:id="@+id/Modulo4Etapa3Pergunta1Linha8"
            android:text="@string/Modulo4Etapa3Pergunta1Linha8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/corTituloLicao"
            android:textSize="12.5sp"
            android:typeface="normal"
            android:fontFamily="@string/fonteQuestao"
            android:layout_marginBottom="5dp"/>

        <TextView
            android:id="@+id/Modulo4Etapa3Pergunta1Linha9"
            android:text="@string/Modulo4Etapa3Pergunta1Linha9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/corTituloLicao"
            android:textSize="12.5sp"
            android:typeface="normal"
            android:fontFamily="@string/fonteQuestao"
            android:layout_marginBottom="5dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal|fill_horizontal">

            <EditText
                android:id="@+id/Modulo4Etapa3Pergunta1Linha10Palavra1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:textSize="13sp"
                android:layout_weight="15"
                android:maxLength="4"
                android:maxLines="1"
                android:width="45dp"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/Modulo4Etapa3Pergunta1Linha10"
                android:text="@string/Modulo4Etapa3Pergunta1Linha6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/corTituloLicao"
                android:textSize="12.5sp"
                android:typeface="normal"
                android:fontFamily="@string/fonteQuestao"
                android:layout_marginBottom="5dp"/>

        </LinearLayout>

        <TextView
            android:id="@+id/Modulo4Etapa3Pergunta1Linha11"
            android:text="@string/Modulo4Etapa3Pergunta1Linha11"
