<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tcc.dagon.opus">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <application
        android:name=".Multidex"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".InicialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity" />
        <activity android:name=".AprenderActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".GerenciarPerfilActivity"
            android:label="@string/title_activity_gerenciar_perfil"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".etapas.EtapasModulo1Activity" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Modulos.Modulo1"
            android:label="@string/title_activity_modulo1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Modulos.Modulo_1"
            android:label="@string/title_activity_modulo_1"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>